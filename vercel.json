{
  "version": 2,
  "builds": [
    {
      "src": "adminova/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "functions": {
    "adminova/wsgi.py": {
      "runtime": "python3.12"
    }
  },
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1",
      "headers": {
        "Cache-Control": "s-maxage=31536000,immutable"
      }
    },
    {
      "src": "/media/(.*)",
      "dest": "/media/$1"
    },
    {
      "src": "/api/mpesa-callback",
      "dest": "adminova/wsgi.py"
    },
    {
      "src": "/(.*)",
      "dest": "adminova/wsgi.py"
    }
  ],
  "env": {
    "PYTHONPATH": "/",
    "DJANGO_SETTINGS_MODULE": "adminova.settings.production"
  }
}